<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="serilog:using:RollingFile" value="Serilog.Sinks.RollingFile" />
    <add key="serilog:write-to:RollingFile.pathFormat" value="log-{Date}.txt" />
    <add key="serilog:write-to:RollingFile.fileSizeLimitBytes" value="1000000" />
    <add key="serilog:write-to:RollingFile.retainedFileCountLimit" value="9" />
  </appSettings>
</configuration>